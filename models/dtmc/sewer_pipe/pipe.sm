dtmc

const double p11;
const double p22;
const double p33;
const double p44;

module pipe

    s : [1..5] init 1;
    
    [] s=1 -> p11 : (s'=1) + 1-p11 : (s'=2);
    [] s=2 -> p22 : (s'=2) + 1-p22 : (s'=3);
    [] s=3 -> p33 : (s'=3) + 1-p33 : (s'=4);
    [] s=4 -> p44 : (s'=4) + 1-p44 : (s'=5);
    [] s=5 -> (s'=5);

endmodule